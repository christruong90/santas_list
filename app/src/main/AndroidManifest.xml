<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.bcit.ass2.truong_chen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddChild">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditChild"
            android:label="@string/edit_child_title"
            android:parentActivityName=".AddChild" />
        <activity
            android:name=".DisplayList"
            android:label="@string/display_list"
            android:parentActivityName=".AddChild"></activity>
        <activity
            android:name=".InsertRow"
            android:label="Add a Child"
            android:parentActivityName=".EditChild">
        </activity>
    </application>

</manifest>